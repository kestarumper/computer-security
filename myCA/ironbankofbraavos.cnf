#
# ironbankofbraavos.cnf
#

[ req ]
prompt = no
distinguished_name = server_distinguished_name
req_extensions = v3_req

[ server_distinguished_name ]
commonName = ironbankofbraavos
stateOrProvinceName = SB
countryName = PL
emailAddress = ironbank@braavos.com
organizationName = Iron Bank of Braavos
organizationalUnitName = IBB Development

[ v3_req ]
basicConstraints = CA:FALSE
keyUsage = nonRepudiation, digitalSignature, keyEncipherment
subjectAltName = @alt_names

[ alt_names ]
DNS.0 = localhost
DNS.1 = ironbankofbraavos